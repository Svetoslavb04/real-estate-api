{
  "info": {
    "name": "Real Estate API",
    "description": "API for managing real estate properties, appointments, and users",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"firstName\": \"John\",\n    \"lastName\": \"Doe\",\n    \"email\": \"john@example.com\",\n    \"password\": \"password123\",\n    \"phone\": \"+1234567890\",\n    \"role\": \"client\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"email\": \"john@example.com\",\n    \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          }
        }
      ]
    },
    {
      "name": "Properties",
      "item": [
        {
          "name": "Create Property",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"title\": \"Luxury Apartment\",\n    \"description\": \"Beautiful apartment in the city center\",\n    \"price\": 250000,\n    \"address\": \"123 Main St\",\n    \"city\": \"New York\",\n    \"bedrooms\": 2,\n    \"bathrooms\": 2,\n    \"area\": 120.5,\n    \"propertyType\": \"apartment\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/properties",
              "host": ["{{baseUrl}}"],
              "path": ["properties"]
            }
          }
        },
        {
          "name": "Get All Properties",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/properties?page=1&limit=10&city=New York&minPrice=200000&maxPrice=300000&propertyType=apartment",
              "host": ["{{baseUrl}}"],
              "path": ["properties"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                },
                {
                  "key": "city",
                  "value": "New York"
                },
                {
                  "key": "minPrice",
                  "value": "200000"
                },
                {
                  "key": "maxPrice",
                  "value": "300000"
                },
                {
                  "key": "propertyType",
                  "value": "apartment"
                }
              ]
            }
          }
        },
        {
          "name": "Get Property by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}"]
            }
          }
        },
        {
          "name": "Update Property",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"price\": 275000,\n    \"description\": \"Updated description\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}"]
            }
          }
        },
        {
          "name": "Delete Property",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Property Images",
      "item": [
        {
          "name": "Upload Image",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/image.jpg"
                },
                {
                  "key": "title",
                  "value": "Front View",
                  "type": "text"
                },
                {
                  "key": "description",
                  "value": "Front view of the property",
                  "type": "text"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}/images",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}", "images"]
            }
          }
        },
        {
          "name": "Get Property Images",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}/images",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}", "images"]
            }
          }
        },
        {
          "name": "Update Image",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"title\": \"Updated Title\",\n    \"description\": \"Updated description\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}/images/{{imageId}}",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}", "images", "{{imageId}}"]
            }
          }
        },
        {
          "name": "Delete Image",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}/images/{{imageId}}",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}", "images", "{{imageId}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Appointments",
      "item": [
        {
          "name": "Create Appointment",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
