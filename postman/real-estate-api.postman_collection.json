            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"appointmentDate\": \"2024-03-20T14:00:00Z\",\n    \"durationMinutes\": 60,\n    \"clientName\": \"John Doe\",\n    \"clientEmail\": \"john@example.com\",\n    \"clientPhone\": \"+1234567890\",\n    \"notes\": \"Interested in viewing the property\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/properties/{{propertyId}}/appointments",
              "host": ["{{baseUrl}}"],
              "path": ["properties", "{{propertyId}}", "appointments"]
